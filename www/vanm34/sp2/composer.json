{
    "name": "vse/mailer-project",
    "require": {
        "php": ">=8",
        "ext-json": "*",
        "davidepastore/slim-validation": "^3.0",
        "monolog/monolog": "^2.9",
        "nyholm/psr7": "^1.8",
        "php-di/php-di": "^6.4",
        "phpmailer/phpmailer": "^6.9.1",
        "slim/http": "^1.3",
        "slim/php-view": "^3.2",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0.0",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
